<h1>Sign Up</h1>

<form class="sign-up" action="<%= user_url %>" method="post">
  <%= auth_token %>

  <label>username
    <input type="text" name="user[username]">
  </label>

  <label>password
    <input type="password" name="user[password]">
  </label>

  <input type="submit" value="Sign Up">
</form>
